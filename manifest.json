{
	"description": "Adds more Shrek in your life. Randomly replaces a single image on 1% of pages with an image of Shrek.",
	"manifest_version": 2,
	"name": "Shrekify",
	"version": "1.0",
	"applications": {
		"gecko": {
			"id": "shrekify@code-moon.com"
		}
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"libs/jquery-3.0.0.min.js",
				"src/shrekify.js"
			],
			"run_at": "document_start"
		}
	],
	"icons": {
		"48": "src/images/icon48.png"
	},
